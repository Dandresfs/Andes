extends base
block titulo
    | Formadores
block itemsNavbarBreadcrumb
    li
        a(href="/region/") Inicio
    li
        a(href="../../../../") {{REGION}}
    li
        a(href="../../../") Andes
    li
        a(href="../../") Administrativo
    li
        a(href="../") CPE
    li(class="active") Obligaciones Contractuales
block body_content
    div(class="container-fluid")
        input(type="hidden",name="id_region",value="{{ID_REGION}}",id="id_region")
        table(id="table",class="table table-striped table-bordered")
            thead
                tr
                    th
                    th Numero
            tbody
                tr
                    th
                    th Numero
block extra_script
    script(src="{% static 'js/cpe_obligaciones_listado.js' %}")