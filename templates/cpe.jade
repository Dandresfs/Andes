extends base
block titulo
    |Tipo de Información

block itemsNavbarBreadcrumb
    li
        a(href="/region/") Inicio
    li
        a(href="../") {{REGION}}
    li(class="active") CPE

block body_content
    div(class="container-fluid")
        div(class="col-xs-12 col-sm-12 col-md-4 col-lg-4 margin-top-50")
            a(href="acceso/",class="text-center")
                div(class="box")
                    div(class="row")
                        div(class="col-xs-12 col-sm-12 col-md-12 col-lg-12 rol")
                            img(src="{% static 'imagenes/acceso.png' %}",class="center-block img-responsive hvr-pop")
                    div(class="row text-center")
                        p Acceso
        div(class="col-xs-12 col-sm-12 col-md-4 col-lg-4 margin-top-50")
            a(href="formacion/",class="text-center")
                div(class="box")
                    div(class="row")
                        div(class="col-xs-12 col-sm-12 col-md-12 col-lg-12 rol")
                            img(src="{% static 'imagenes/formacion.png' %}",class="center-block img-responsive hvr-pop")
                    div(class="row text-center")
                        p Formación
        div(class="col-xs-12 col-sm-12 col-md-4 col-lg-4 margin-top-50")
            a(href="administrativo/",class="text-center")
                div(class="box")
                    div(class="row")
                        div(class="col-xs-12 col-sm-12 col-md-12 col-lg-12 rol")
                            img(src="{% static 'imagenes/administrativo.png' %}",class="center-block img-responsive hvr-pop")
                    div(class="row text-center")
                        p Administrativo y Financiero
    div(class="container-fluid margin-top-50")