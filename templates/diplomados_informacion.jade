extends base

block titulo
    | {{DIPLOMADO}}

block itemsNavbarBreadcrumb
    li
        a(href="/region/") Inicio
    li
        a(href="../../../") {{REGION}}
    li
        a(href="../../") Diplomados
    li(class="active") {{DIPLOMADO}}
block body_content
    div(class="container-fluid")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-4 margin-top-50")
            a(href="participantes/",class="text-center")
                div(class="box")
                    div(class="col-xs-12 col-sm-12 col-md-12 col-lg-12 diplomado")
                        img(src="{% static 'imagenes/participantes.png' %}",class="center-block img-responsive hvr-pop")
                    p(class="text-center") Participantes
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-4 margin-top-50")
            a(href="actividades/",class="text-center")
                div(class="box")
                    div(class="col-xs-12 col-sm-12 col-md-12 col-lg-12 diplomado")
                        img(src="{% static 'imagenes/actividades.png' %}",class="center-block img-responsive hvr-pop")
                    p(class="text-center") Actividades
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-4 margin-top-50")
            a(href="proyectos/",class="text-center")
                div(class="box")
                    div(class="col-xs-12 col-sm-12 col-md-12 col-lg-12 diplomado")
                        img(src="{% static 'imagenes/proyectos.png' %}",class="center-block img-responsive hvr-pop")
                    p(class="text-center") Proyectos