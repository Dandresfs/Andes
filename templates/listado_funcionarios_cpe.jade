extends base
block titulo
    | Funcionarios
block itemsNavbarBreadcrumb
    li
        a(href="/region/") Inicio
    li
        a(href="../../../") {{REGION}}
    li
        a(href="../../") Cpe
    li
        a(href="../",style="text-transform: capitalize;") {{EJE}}
    li(class="active") Funcionarios
block body_content
    div(class="container-fluid")
        input(type="hidden",name="id_region",value="{{ID_REGION}}",id="id_region")
        input(type="hidden",name="id_eje",value="{{ID_EJE}}",id="id_eje")
        table(id="table",class="table table-striped table-bordered")
            thead
                tr
                    th
                    th Nombre
                    th Cedula
                    th Celular
                    th Correo
                    th Inicio de Contrato
                    th Fin de Contrato
            tbody
                tr
                    th
                    th Nombre
                    th Cedula
                    th Celular
                    th Correo
                    th Inicio de Contrato
                    th Fin de Contrato
block extra_script
    script(src="{% static 'js/listado_funcionario_eje.js' %}")