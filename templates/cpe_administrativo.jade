extends base
block titulo
    | CPE
block itemsNavbarBreadcrumb
    li
        a(href="/region/") Inicio
    li
        a(href="../../../") {{REGION}}
    li
        a(href="../../") Andes
    li
        a(href="../") Administrativo
    li(class="active") CPE
block body_content
    div(class="container-fluid")
        div(class="col-xs-12 col-sm-12 col-md-4 col-lg-4 margin-top-50")
            a(href="obligaciones/",class="text-center")
                div(class="box")
                    div(class="row")
                        div(class="col-xs-12 col-sm-12 col-md-12 col-lg-12 rol")
                            img(src="{% static 'imagenes/contrato.png' %}",class="center-block img-responsive hvr-pop")
                    div(class="row text-center")
                        p Obligaciones Contractuales
        div(class="col-xs-12 col-sm-12 col-md-4 col-lg-4 margin-top-50")
            a(href="informes/",class="text-center")
                div(class="box")
                    div(class="row")
                        div(class="col-xs-12 col-sm-12 col-md-12 col-lg-12 rol")
                            img(src="{% static 'imagenes/administrativo.png' %}",class="center-block img-responsive hvr-pop")
                    div(class="row text-center")
                        p Informe Administrativo y Financiero
        div(class="col-xs-12 col-sm-12 col-md-4 col-lg-4 margin-top-50")
            a(href="auxiliares/",class="text-center")
                div(class="box")
                    div(class="row")
                        div(class="col-xs-12 col-sm-12 col-md-12 col-lg-12 rol")
                            img(src="{% static 'imagenes/auxiliar.png' %}",class="center-block img-responsive hvr-pop")
                    div(class="row text-center")
                        p Carga Auxiliares
    div(class="container-fluid margin-top-50")