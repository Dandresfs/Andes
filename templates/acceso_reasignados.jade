extends base
block titulo
    | Gestores
block itemsNavbarBreadcrumb
    li
        a(href="/region/") Inicio
    li
        a(href="../../../") {{REGION}}
    li
        a(href="../../") Andes
    li
        a(href="../") Acceso
    li(class="active") Radicados reasignados
block body_content
    div(class="container-fluid")
        input(type="hidden",name="id_region",value="{{ID_REGION}}",id="id_region")
        table(id="table",class="table table-striped table-bordered")
            thead
                tr
                    th
                    th Nombre
                    th Cedula
            tbody
                tr
                    th
                    th Nombre
                    th Cedula
block extra_script
    script(src="{% static 'js/listado_radicados_reasignados.js' %}")