extends base
block titulo
    | Formadores
block itemsNavbarBreadcrumb
    li
        a(href="/region/") Inicio
    li
        a(href="../../../../") {{REGION}}
    li
        a(href="../../../") Andes
    li
        a(href="../../") Financiero
    li
        a(href="../") Formadores
    li(class="active") Tipo {{ID_TIPO}}
block body_content
    div(class="container-fluid")
        input(type="hidden",name="id_region",value="{{ID_REGION}}",id="id_region")
        input(type="hidden",name="id_tipo",value="{{ID_TIPO}}",id="id_tipo")
        table(id="table",class="table table-striped table-bordered")
            thead
                tr
                    th
                    th Nombre
                    th Cedula
                    th Celular
                    th Correo
                    th Inicio de Contrato
                    th Fin de Contrato
            tbody
                tr
                    th
                    th Nombre
                    th Cedula
                    th Celular
                    th Correo
                    th Inicio de Contrato
                    th Fin de Contrato
block extra_script
    script(src="{% static 'js/listado_formadores_financiero.js' %}")